                            <% if (character.equipments && character.equipments.length > 0) { %>
                                <% character.equipments.forEach((equipment, index) => { %>
                                    <div class="equipamento-lista">
                                        <label>Nome:</label>
                                        <input type="text" id="equip<%= index + 1 %>" 
                                            name="equipments[<%= index %>][name]" 
                                            value="<%= equipment.name %>">

                                        <label>Peso:</label>
                                        <input type="text" id="peso<%= index + 1 %>" 
                                            name="equipments[<%= index %>][weight]" 
                                            value="<%= equipment.weight %>">

                                        <textarea id="descricao_peso<%= index + 1 %>" 
                                                name="equipments[<%= index %>][description]" 
                                                rows="2"><%= equipment.description %></textarea>
                                    </div>
                                <% }) %>
                            <% } else { %>
                                <div class="equipamento-lista">
                                    <label>Nome:</label>
                                    <input type="text" id="equip1" name="equipments[0][name]">

                                    <label>Peso:</label>
                                    <input type="text" id="peso1" name="equipments[0][weight]">

                                    <textarea id="descricao_peso1" name="equipments[0][description]" rows="2">Descrição:</textarea>
                                </div>
                            <% } %>
                        </div>
                        <div class="mais-menos">
                            <button id="equip-mais">+</button>
                            <button id="equip-menos"> - </button>
                        </div>
                    </div>
        
                    <div class="armadura-defesa">
                        <h2>Armadura e Defesas</h2>
                        <div class="flex-ad">
                            <div class="grid-armadura">
                                <div class="cabeca">
                                    <label for="cabecaa">Cabeça:</label>
                                    <input type="number" id="cabecaa" name="defense[head][armour]" placeholder="Armadura"
                                           value="<%= character.defense.head.armour %>">
                                    <input type="number" id="cabecat" name="defense[head][totalArmour]" class="armadura-total"
                                           value="<%= character.defense.head.totalArmour %>">
                                </div>
                            
                                <div class="bracoe">
                                    <label for="bracoea">Braço Esquerdo:</label>
                                    <input type="number" id="bracoea" name="defense[leftArm][armour]" placeholder="Armadura"
                                           value="<%= character.defense.leftArm.armour %>">
                                    <input type="number" id="bracoet" name="defense[leftArm][totalArmour]" class="armadura-total"
                                           value="<%= character.defense.leftArm.totalArmour %>">
                                </div>
                            
                                <div class="bracod">
                                    <label for="bracoda">Braço Direito:</label>
                                    <input type="number" id="bracoda" name="defense[rightArm][armour]" placeholder="Armadura"
                                           value="<%= character.defense.rightArm.armour %>">
                                    <input type="number" id="bracodt" name="defense[rightArm][totalArmour]" class="armadura-total"
                                           value="<%= character.defense.rightArm.totalArmour %>">
                                </div>
                            
                                <div class="corpo">
                                    <label for="corpoa">Corpo:</label>
                                    <input type="number" id="corpoa" name="defense[body][armour]" placeholder="Armadura"
                                           value="<%= character.defense.body.armour %>">
                                    <input type="number" id="corpot" name="defense[body][totalArmour]" class="armadura-total"
                                           value="<%= character.defense.body.totalArmour %>">
                                </div>
                            
                                <div class="pernae">
                                    <label for="pernaea">Perna Esquerda:</label>
                                    <input type="number" id="pernaea" name="defense[leftLeg][armour]" placeholder="Armadura"
                                           value="<%= character.defense.leftLeg.armour %>">
                                    <input type="number" id="pernaet" name="defense[leftLeg][totalArmour]" class="armadura-total"
                                           value="<%= character.defense.leftLeg.totalArmour %>">
                                </div>
                            
                                <div class="pernad">
                                    <label for="pernada">Perna Direita:</label>
                                    <input type="number" id="pernada" name="defense[rightLeg][armour]" placeholder="Armadura"
                                           value="<%= character.defense.rightLeg.armour %>">
                                    <input type="number" id="pernadt" name="defense[rightLeg][totalArmour]" class="armadura-total"
                                           value="<%= character.defense.rightLeg.totalArmour %>">
                                </div>
                            </div>
                            <div class="vida-condicao">
                                <div class="vida">
                                    <h3>Vida</h3>
                                    <label for="vida_total">Total:</label>
                                    <input type="number" id="vida_total" name="health[total]" min="0" 
                                           value="<%= character.health.total %>">
                                
                                    <label for="vida_atual">Atual:</label>
                                    <input type="number" id="vida_atual" name="health[current]" min="0" 
                                           value="<%= character.health.current %>">
                                
                                    <label for="dano_critico">Dano Crítico:</label>
                                    <input type="number" id="dano_critico" name="health[criticalDamage]" min="0" max="10" 
                                           value="<%= character.health.criticalDamage %>">
                                </div>
                                <div class="condicoes">
                                    <h3>Condições</h3>
                                    <% character.conditions.forEach((condition, index) => { %>
                                        <input type="text" id="condicao<%= index + 1 %>" name="conditions[<%= index %>][name]" 
                                               placeholder="Nome da Condição" value="<%= condition.name %>">
                                        
                                        <textarea id="descricao_condicao<%= index + 1 %>" name="conditions[<%= index %>][description]" rows="1">
                                            <%= condition.description %>
                                        </textarea>
                                    <% }); %>
                                </div>
                            </div>
                        </div>
                    </div>
        
                    <div class="movimento-fadiga">
                        <div class="movimento">
                            <h2>Movimento</h2>
                            <div class="grid-movimento">
                                <div class="movimento-cell">
                                    <label for="movi_metade">Metade:</label>
                                    <input type="number" id="movi_metade" name="movement[half]" 
                                        value="<%= character.movement.half %>">
                                </div>
                                <div class="movimento-cell">
                                    <label for="movi_total">Total:</label>
                                    <input type="number" id="movi_total" name="movement[full]" 
                                        value="<%= character.movement.full %>">
                                </div>
                                <div class="movimento-cell">
                                    <label for="movi_disparada">Disparada:</label>
                                    <input type="number" id="movi_disparada" name="movement[charge]" 
                                        value="<%= character.movement.charge %>">
                                </div>
                                <div class="movimento-cell">
                                    <label for="movi_corrida">Corrida:</label>
                                    <input type="number" id="movi_corrida" name="movement[run]" 
                                        value="<%= character.movement.run %>">
                                </div>
                            </div>
                        </div>
        
                        <div class="fadiga">
                            <h2>Fadiga</h2>
                            <div class="grid-fadiga">
                                <div>
                                    <label for="fadiga_limite">Limite:</label>
                                    <input type="number" id="fadiga_limite" name="fatigue[limit]" min="0"
                                           value="<%= character.fatigue.limit %>">
                                </div>
                                <div>
                                    <label for="fadiga_atual">Atual:</label>
                                    <input type="number" id="fadiga_atual" name="fatigue[current]" min="0"
                                           value="<%= character.fatigue.current %>">
                                </div> 
                            </div>
                        </div>
                    </div>
        
                    <div id="psiquico" class="psiquico">
                        <h2>Poderes Psíquicos</h2>
                        <div class="psi-nivel">
                            <label for="pp_psi">Psi-Nível:</label>
                            <input type="number" id="pp_psi" name="psiLevel" value="<%= character.psiLevel %>" min="0">
                        </div>
                        <div id="poder-container">
                            <% if (character.powers && character.powers.length > 0) { %>
                                <% character.powers.forEach((power, index) => { %>
                                    <div class="lista-poder">
                                        <label>Poder:</label>
                                        <input type="text" id="pp_poder<%= index + 1 %>" name="powers[<%= index %>][name]" 
                                               placeholder="Nome do Poder" value="<%= power.name %>">
                        
                                        <input type="text" id="pp_descricao<%= index + 1 %>" name="powers[<%= index %>][description]" 
                                               class="pp-descricao" placeholder="Descrição:" value="<%= power.description %>">
                                    </div>
                                <% }) %>
                            <% } else { %>
                                <div class="lista-poder">
                                    <label>Poder:</label>
                                    <input type="text" id="pp_poder1" name="powers[0][name]" placeholder="Nome do Poder">
                                    <input type="text" id="pp_descricao1" name="powers[0][description]" class="pp-descricao" placeholder="Descrição:">
                                </div>
                            <% } %>
                        </div>
                        <div class="mais-menos">
                            <button id="poder-mais">+</button>
                            <button id="poder-menos"> - </button>
                        </div>
                    </div>

                    <div class="container-mostrar-esconder-poderes">
                        <button id="mostrar-esconder-poder">Esconder Poderes</button>
                    </div>
                    
                    <div class="notas">
                        <h2>Notas</h2>
                        <textarea id="notas" name="notes" rows="1"><%= character.notes %></textarea>
                    </div>
                </div>
            </div>
            <div class="submit">
                <button type="submit">
                    SALVAR
                </button>
            </div>             
        </form>
    </div>
    <script src="/detalhes-personagem.js"></script>
</body>