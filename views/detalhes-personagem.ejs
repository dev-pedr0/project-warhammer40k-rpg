                            <input type="number" id="tdeg" name="exp[expSpent]" value="<%= character.exp.expSpent %>" min="0" step="1">
                        </div>
                        <div class="xp-pd-1">
                            <h2>Pontos de Destino</h2>
        
                            <label for="fatePoints[limit]">Limite</label>
                            <input type="number" id="limite" name="fatePoints[limit]" min="0" step="1" value="<%= character.fatePoints.limit %>">
            
                            <label for="fatePoints[current]">Atual</label>
                            <input type="number" id="atual" name="fatePoints[current]"min="0" step="1" value="<%= character.fatePoints.current %>">
                        </div>
