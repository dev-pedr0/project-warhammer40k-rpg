<!DOCTYPE html>
<html lang="pt">
<head>
    <meta charset="UTF-8">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <link rel="stylesheet" href="/detalhes-personagem.css">
    <title>Document</title>
</head>
<body>
    <div class="ficha">
        <a class="link-retornar" href="/user/<%= user.id %>/lista-personagem">
            <img src="/imgs/home.png" alt="retornar-inicio" title="Retornar">
        <!-- Créditos da imagem: Dave Gandy -->
        </a>
        <form action="/character-update/<%= character._id %>" method="post">
            <div class="nome">
                <%= character.characterName %>
            </div>
            <div class="dados-iniciais">
                <div>
                    <label for="homePlanet">Planeta de Origem:</label>
                    <input type="text" id="pdo" name="homePlanet" value="<%= character.homePlanet %>">
                </div>
                <div>
                    <label for="background">Background:</label>
                    <input type="text" id="background" name="background" value="<%= character.background %>">
                </div>
                <div>
                    <label for="role">Função:</label>
                    <input type="text" id="função" name="role" value="<%= character.role %>">
                </div>
                <div>
                    <label for="eliteAdvance">Avanço de Elite:</label>
                    <input type="text" id="ade" name="eliteAdvance" value="<%= character.eliteAdvance %>">
                </div>
                <div>
                    <label for="destiny">Destino:</label>
                    <input type="text" id="destino" name="destiny" value="<%= character.destiny %>">
                </div>
            </div>

            <div class="divisor-ficha">
                <div class="coluna">
                    <div class="aliados-inimigos">
                        <div class="lista">
                            <h2>Alidados</h2>
                            <div id="area-aliados">
                                <div class="container-aliados">
                                    <% if (character.allies && character.allies.length > 0) { %>
                                        <% character.allies.forEach((ally, index) => { %>
                                            <div class="input-aliado">
                                                <input type="text" id="al<%= index %>" name="allies[<%= index %>]" value="<%= ally %>">
                                            </div>
                                        <% }) %>
                                    <% } else { %>
                                        <input type="text" id="al0" name="allies[0]">
                                    <% } %>
                                </div>
                            </div>
                            <div class="botoes">
                                <button id="mais-alidados">+</button>
                                <button id="menos-aliados"> - </button>
                            </div>
                        </div>
                        <div class="lista">
                            <h2>Inimigos</h2>
                            <div id="area-inimigos">
                                <div class="container-inimigos">
                                    <% if (character.enemies && character.enemies.length > 0) { %>
                                        <% character.enemies.forEach((enemy, index) => { %>
                                            <div class="input-aliado">
                                                <input type="text" id="ini<%= index %>" name="enemies[<%= index %>]" value="<%= enemy %>">
                                            </div>
                                        <% }) %>
                                    <% } else { %>
                                        <input type="text" id="ini0" name="enemies[0]">
                                    <% } %>
                                </div>
                            </div>
                            <div class="botoes">
                                <button id="mais-inimigos">+</button>
                                <button id="menos-inimigos"> - </button>
                            </div>
                        </div>
                    </div>
        
