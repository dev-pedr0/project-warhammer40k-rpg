                                <div class="movimento-cell">
                                    <label for="movi_corrida">Corrida:</label>
                                    <input type="number" id="movi_corrida" name="movi_corrida">
                                </div>
                            </div>
                        </div>
        
                        <div class="fadiga">
                            <h2>Fadiga</h2>
                            <div class="grid-fadiga">
