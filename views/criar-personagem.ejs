<body>
    <div class="ficha">
        <form action="/character" method="post">
            <div class="nome">
                <label>Nome: </label>
                <input type="text" name="characterName">
                <input type="hidden" name="userId" value="<%= user._id %>" />
            </div>
            <div class="dados-iniciais">
                <div>
                    <label for="homePlanet">Planeta de Origem:</label>
                    <input type="text" id="pdo" name="homePlanet">
                </div>
                <div>
                    <label for="background">Background:</label>
                    <input type="text" id="background" name="background">
                </div>
                <div>
                    <label for="role">Função:</label>
                    <input type="text" id="função" name="role">
                </div>
                <div>
                    <label for="eliteAdvance">Avanço de Elite:</label>
                    <input type="text" id="ade" name="eliteAdvance">
                </div>
                <div>
                    <label for="destiny">Destino:</label>
                    <input type="text" id="destino" name="destiny">
                </div>
            </div>

            <div class="divisor-ficha">
                <div class="coluna">
                    <div class="aliados-inimigos">
                        <div class="lista">
                            <h2>Alidados</h2>
                            <div id="area-aliados">
                                <div class="container-aliados">
                                    <input type="text" id="al0" name="allies[0]">
                                </div>
                            </div>
                            <div class="botoes">
                                <button id="mais-alidados">+</button>
                                <button id="menos-aliados"> - </button>
                            </div>
                        </div>
                        <div class="lista">
                            <h2>Inimigos</h2>
                            <div id="area-inimigos">
                                <div class="container-inimigos">
                                    <input type="text" id="ini0" name="enemies[0]">
                                </div>
                            </div>
                            <div class="botoes">
                                <button id="mais-inimigos">+</button>
                                <button id="menos-inimigos"> - </button>
                            </div>
                        </div>
                    </div>
        
