                                <div class="arma-dano">
                                    <label>Dano:</label>
                                    <input type="text" id="arma_dano_1" name="arma_dano_1">
                                </div>
                                <div class="arma-pen">
                                    <label>Pen:</label>
                                    <input type="text" id="arma_pen_1" name="arma_pen_1">
