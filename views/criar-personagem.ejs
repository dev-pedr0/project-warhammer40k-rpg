                                    <label for="fadiga_limite">Limite:</label>
                                    <input type="number" id="fadiga_limite" name="fadiga_limite" min="0">
                                </div>
                                <div>
                                    <label for="fadiga_atual">Atual:</label>
                                    <input type="number" id="fadiga_atual" name="fadiga_atual" min="0">
                                </div> 
                            </div>
                        </div>
                    </div>
        
                    <div id="psiquico" class="psiquico">
                        <h2>Poderes Psíquicos</h2>
                        <div class="psi-nivel">
