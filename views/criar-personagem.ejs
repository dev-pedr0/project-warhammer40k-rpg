                                </div>
                                <div class="bracod">
                                    <label for="bracoda">Braço Direito:</label>
                                    <input type="number" id="bracoda" name="bracoda" placeholder="Armadura">
                                    <input type="number" id="bracodt" name="bracodt" class="armadura-total">
                                </div>
                                <div class="corpo">
                                    <label for="corpoa">Corpo:</label>
                                    <input type="number" id="corpoa" name="corpoa" placeholder="Armadura">
                                    <input type="number" id="corpot" name="corpot" class="armadura-total">
                                </div>
                                <div class="pernae">
                                    <label for="pernaea">Perna Esquerda:</label>
                                    <input type="number" id="pernaea" name="pernaea" placeholder="Armadura">
                                    <input type="number" id="pernaet" name="pernaet" class="armadura-total">
                                </div>
                                <div class="pernad">
                                    <label for="pernada">Perna Direita:</label>
                                    <input type="number" id="pernada" name="pernada" placeholder="Armadura">
