                                    <textarea id="descricao_condicao2" name="descricao_condicao2" rows="1">Descrição:</textarea>
                                    <input type="text" id="condicao3" name="condicao3" placeholder="Nome da Condição">
                                    <textarea id="descricao_condicao3" name="descricao_condicao3" rows="1">Descrição:</textarea>
                                </div>
                            </div>
                        </div>
                    </div>
        
