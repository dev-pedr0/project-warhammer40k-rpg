                                <label>Poder:</label>
                                <input type="text" id="pp_poder1" name="pp_poder1" placeholder="Nome do Poder">
                                <input type="text" id="pp_descricao1" name="pp_descricao1" class="pp-descricao" placeholder="Descrição:">
                            </div>
                        </div>
                        <div class="mais-menos">
                            <button id="poder-mais">+</button>
                            <button id="poder-menos"> - </button>
                        </div>
                    </div>

                    <div class="container-mostrar-esconder-poderes">
                        <button id="mostrar-esconder-poder">Esconder Poderes</button>
                    </div>
                    
                    <div class="notas">
                        <h2>Notas</h2>
                        <textarea id="notas" name="notas" rows="1">Notas:</textarea>
                    </div>
